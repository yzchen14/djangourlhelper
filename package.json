{
  "name": "djangourlhelper",
  "displayName": "Django URL Helper",
  "description": "A VS Code extension to help manage and navigate Django URLs",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/djangourlhelper.git"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:djangoUrlExplorer",
    "onCommand:djangourlhelper.refresh"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "django-url-helper",
          "title": "Django URL Helper",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "django-url-helper": [
        {
          "id": "djangoUrlExplorer",
          "name": "URL Explorer",
          "contextualTitle": "Django URL Explorer"
        }
      ]
    },
    "commands": [
      {
        "command": "djangourlhelper.refresh",
        "title": "Refresh Django URLs",
        "icon": "$(refresh)",
        "category": "Django URL Helper"
      },
      {
        "command": "djangourlhelper.copyUrl",
        "title": "Copy URL",
        "category": "Django URL Helper"
      },
      {
        "command": "djangourlhelper.quickInsertUrl",
        "title": "Quick Insert URL",
        "category": "Django URL Helper"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "djangourlhelper.refresh",
          "when": "view == djangoUrlExplorer",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "typescript": "^5.0.4",
    "@vscode/test-electron": "^2.3.0"
  }
}
